{
  "name": "kjf-ngx",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --configuration es5",
    "build": "ng build --prod",
    "test": "ng test",
    "pretty": "pretty-quick --staged",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --fix",
    "e2e": "ng e2e",
    "test:lib": "ng test kjf-ngx-lib --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "build:kjf-ngx": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build kjf-ngx-lib --configuration production",
    "build:kjf-ngx:ivy": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build kjf-ngx-lib"
  },
  "dependencies": {
    "@angular/animations": "~10.1.3",
    "@angular/cdk": "^11.2.1",
    "@angular/common": "~10.1.3",
    "@angular/compiler": "~10.1.3",
    "@angular/core": "~10.1.3",
    "@angular/forms": "~10.1.3",
    "@angular/platform-browser": "~10.1.3",
    "@angular/platform-browser-dynamic": "~10.1.3",
    "@angular/router": "~10.1.3",
    "lodash-es": "^4.17.15",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "~10.1.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@types/chai": "^4.2.15",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "codelyzer": "^6.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.1.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
